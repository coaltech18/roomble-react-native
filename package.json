{
  "name": "roomble",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend:start": "cd backend && func start",
    "typecheck": "tsc --noEmit",
    "ngrok:setup": "node setup-ngrok.js",
    "ngrok:start": "ngrok http 4000",
    "ngrok:test": "node test-ngrok.js",
    "server:start": "cd server && npm run dev",
    "dev:full": "concurrently \"npm run server:start\" \"npm run ngrok:start\"",
    "deploy:check": "node deploy-to-render.js",
    "deploy:render": "echo 'Follow RENDER_DEPLOYMENT.md for detailed instructions' && node deploy-to-render.js",
    "deploy:mobile": "node deploy-mobile.js",
    "build:android": "eas build --profile preview --platform android",
    "build:ios": "eas build --profile preview --platform ios",
    "build:all": "eas build --profile preview --platform all"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@microsoft/signalr": "^9.0.6",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.24",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.11.0",
    "expo": "~53.0.20",
    "expo-application": "^6.1.5",
    "expo-device": "^7.1.4",
    "expo-haptics": "^14.1.4",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "^7.1.7",
    "expo-location": "^18.1.6",
    "expo-notifications": "^0.31.4",
    "expo-secure-store": "^14.2.3",
    "expo-status-bar": "~2.2.3",
    "lodash": "^4.17.21",
    "lodash.isequal": "^4.5.0",
    "mongodb": "^6.18.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-deck-swiper": "^2.0.18",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "^1.20.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "zod": "^4.0.15",
    "expo-font": "~13.3.2",
    "expo-dev-client": "~5.2.4",
    "react-dom": "19.0.0",
    "react-native-web": "^0.20.0",
    "@expo/metro-runtime": "~5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
